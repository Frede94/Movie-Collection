<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="movie_project.gui.AddCategoryController">
   <children>
      <Label layoutX="182.0" layoutY="31.0" text="New Category">
         <font>
            <Font name="System Bold" size="32.0" />
         </font>
      </Label>
      <Label layoutX="145.0" layoutY="164.0" text="Name:">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="categoryName" layoutX="215.0" layoutY="166.0" />
      <Button fx:id="saveCategoryBtn" layoutX="156.0" layoutY="280.0" mnemonicParsing="false" onAction="#saveCategoryOnAction" prefHeight="38.0" prefWidth="85.0" text="Save" />
      <Button fx:id="cancelCategoryBtn" layoutX="319.0" layoutY="280.0" mnemonicParsing="false" onAction="#cancelCategoryOnAction" prefHeight="38.0" prefWidth="85.0" text="Cancel" />
   </children>
</AnchorPane>
